    "prisma:studio": "prisma studio --schema=../api/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "@repo/ui": "*",
    "next": "16.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "autoprefixer": "^10.4.23",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "postcss": "^8.5.6",
    "prisma": "^6.19.2",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.2"
  }
