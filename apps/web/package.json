{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "next typegen && tsc --noEmit",
    "prisma:generate": "prisma generate --schema=../api/prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=../api/prisma/schema.prisma",
